{
    "name": "altcase",
    "version": "0.1.0",
    "description": "Convert text to alternating case",
    "exports": {
        "default": "./dist/index.mjs"
    },
    "files": [
        "dist/index.mjs"
    ],
    "engines": {
        "node": ">=14.16"
    },
    "repository": "https://github.com/MarkusJx/altcase",
    "scripts": {
        "start": "node --enable-source-maps dist/index.prod.min.js",
        "prestart": "npm run build",
        "build": "tsc",
        "format": "prettier --write ."
    },
    "bin": {
        "altcase": "./dist/index.mjs"
    },
    "keywords": [
        "shell",
        "command-line-utility"
    ],
    "author": "MarkusJx",
    "license": "MIT",
    "type": "module",
    "devDependencies": {
        "@types/inquirer": "^9.0.3",
        "@types/node": "^20.3.1",
        "@types/yargs": "^17.0.24",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "clipboardy": "^3.0.0",
        "conf": "^11.0.1",
        "inquirer": "^9.2.7",
        "yargs": "^17.7.2"
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5"
    }
}
